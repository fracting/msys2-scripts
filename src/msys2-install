#!/bin/sh

set -e
set -x

. /etc/msys2-env

DISPLAY=:88.0 WINEDEBUG=-all wineboot # hack - wineboot without display, create wineprefix in a quick way, workaround Travis CI timeout (FIXME)

cd $WINEPREFIX/drive_c
wget http://sourceforge.net/projects/msys2/files/Base/i686/msys2-base-i686-20150512.tar.xz
tar xf msys2-base-*.tar.xz
